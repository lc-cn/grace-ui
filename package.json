{
	"name": "grace-ui",
	"version": "0.1.1",
	"files": [
		"lib"
	],
	"main": "./lib/index.js",
	"module": "./lib/index.esm.js",
	"types": "./lib/index.d.ts",
	"style": "./lib/theme-default/index.css",
	"exports": {
		".": {
			"import": "./lib/index.esm.js",
			"require": "./lib/index.js"
		},
		"./*": "./*"
	},
	"workspaces": [
		"packages/*",
		"docs"
	],
	"scripts": {
		"new": "bash ./script/template.sh",
		"clean": "rimraf ./lib",
		"dev": "cd example && npm run dev",
		"test": "echo \"Error: no test specified\" && exit 1",
		"register:components": "vitepress-rc",
		"docs:dev": "vitepress dev docs",
		"docs:build": "vitepress build docs",
		"docs:serve": "vitepress serve docs",
		"build": "npm run clean && npm run build:theme && npm run build:package && npm run build:package:dts",
		"build:theme": "gulp build -f build/gulpfile.prod.js",
		"build:theme:rem": "gulp build -f build/gulpfile.rem.js",
		"build:package": "rollup -c build/rollup.config.ts",
		"build:package:dts": "rollup -c build/rollup.config.dts.ts"
	},
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@rollup/plugin-node-resolve": "^13.1.3",
		"@types/node": "^17.0.21",
		"@vitejs/plugin-vue": "^2.2.4",
		"@vue/tsconfig": "^0.1.3",
		"cz-conventional-changelog": "^3.3.0",
		"cz-customizable": "^6.3.0",
		"del": "^6.0.0",
		"gulp": "^4.0.2",
		"gulp-autoprefixer": "^8.0.0",
		"gulp-cssmin": "^0.2.0",
		"gulp-postcss": "^9.0.1",
		"gulp-sass": "^5.1.0",
		"rimraf": "^3.0.2",
		"rollup": "^2.69.2",
		"rollup-plugin-terser": "^7.0.2",
		"rollup-plugin-typescript2": "^0.31.2",
		"rollup-plugin-vue": "^6.0.0",
		"sass": "^1.49.9",
		"typescript": "^4.6.2",
		"vite-plugin-inspect": "^0.4.3",
		"vitepress-theme-demoblock": "^1.4.2"
	},
	"dependencies": {
		"vitepress": "^0.22.3",
		"vue": "^3.2.31"
	}
}
